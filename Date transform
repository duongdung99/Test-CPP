#include <iostream>
#include <vector>
#include <regex>
using namespace std;

vector<string> transformDateFormat(const vector<string>& dates)
{
    vector<string> result;
    regex pattern("(\\d{4})/(\\d{2})/(\\d{2})|(\\d{2})-(\\d{2})-(\\d{4})|(\\d{8})|(\\d{2})/(\\d{2})/(\\d{4})");
    smatch smatch;
    for (const auto& date : dates)
    {
        if (regex_match(date, smatch, pattern))
        {
            if (smatch[1].matched)
            {
                result.push_back(smatch[1].str() + smatch[2].str() + smatch[3].str());
            }
            else if (smatch[4].matched)
            {
                result.push_back(smatch[6].str() + smatch[4].str() + smatch[5].str());
            }
            else if (smatch[7].matched) 
            {
                result.push_back(smatch[7].str());
            }
            else if (smatch[8].matched)
            {
                result.push_back(smatch[10].str() + smatch[9].str() + smatch[8].str());
            }
        }
    }
    return result;
}

#ifndef RunTests

int main()
{
    vector<string> dates = { "2010/02/20", "19/12/2016", "11-18-2012", "20130720" };
    vector<string> reformattedDates = transformDateFormat(dates);
    for (auto const& reformattedDate : reformattedDates)
    {
        cout << reformattedDate << endl;
    }
}

#endif
